{
  "name": "eslint-plugin-benchmarking-test",
  "version": "0.0.1",
  "description": "This projects attempts to introduce vulnerabilities that can be caught by different static analysers and provides a repository for the oSSF benchmarking tool.",
  "main": "./lib/index.js",
  "files": [
    "lib/**"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:tikki100/eslint-plugin-benchmarking-test.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./lib && mkdir ./lib && mkdir ./lib/templates && cp -R ./src/templates/ ./lib/templates/ && tsc --project ./tsconfig.build.json",
    "watch": "yarn build --watch",
    "prepublishOnly": "yarn build",
    "eslint:files": "eslint",
    "prepare": "husky install",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-comment-length": "^0.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.1",
    "tsc-files": "^1.1.3",
    "typescript": "^4.6.3"
  }
}
